  "scripts": {
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "peerDependencies": {
    "astro": "^5.0.0",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "astro": "^5.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.0"
  },
  "keywords": ["astro", "components", "tailwind"],
  "license": "UNLICENSED"
}
